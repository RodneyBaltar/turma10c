package decisao;

import javax.swing.JOptionPane;

public class DesafioComposta {

	public static void main(String[] args) {

		String veiculo = JOptionPane.showInputDialog("Nome do veiculo").toUpperCase();

		byte capacidade = Byte.parseByte(JOptionPane.showInputDialog ("Capacidade"));

		if (capacidade ==2) {

			System.out.println("Veiculo Categoria A:" + capacidade);

		}else if (capacidade >7) {

			System.out.println("Veiculo Categoria C:" + capacidade);
		}
	}
}
